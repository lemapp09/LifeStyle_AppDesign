.navigation-dropdown {
    position: absolute;
    width: 600px;
    height: 50px;
    bottom: 41px;
    left: 74px;
    -unity-font-definition: resource('FlatSkin/Font/Roboto-Black');
    font-size: 36px;
    border-radius: 25px;
    background-color: rgba(152, 114, 114, 0);
}

.navigation-dropdown .unity-base-field__input {
    color: white;
    padding-left: 8px;
}

.news-container {
    height: 70%;
    margin-top: 20px;
    width: 90%;
    max-width: 90%;
}

.news-article {
    background-color: rgba(0, 0, 0, 0.5);
    border-radius: 10px;
    margin: 10px;
    padding: 15px;
}

.news-title {
    font-size: 36px;
    -unity-font-style: bold;
    color: white;
    white-space: normal;
}

.news-description {
    font-size: 24px;
    color: white;
    white-space: normal;
}

.news-link-icon {
    background-image: url("project://database/Assets/Art/App%20Design/newslink.png?fileID=21300000&guid=311dee57c82d0407a89cec21c7eede3b&type=3#newslink");
    width: 32px;
    height: 32px;
    -unity-background-scale-mode: scale-to-fit;
    position: absolute;
    bottom: 10px;
    right: 10px;
}

.news-link-icon:hover {
    scale: 1.1;
}

.tic-tac-toe-container {
    width: 625px;
    height: 525px;
    flex-wrap: wrap;
    flex-direction: row;
    margin-bottom: 50px;
    align-items: center;
    align-content: auto;
}

.tic-tac-toe-cell {
    width: 150px;
    height: 150px;
    font-size: 100px;
    -unity-font-style: bold;
    background-color: rgba(255, 255, 255, 0.2);
    border-color: white;
    border-width: 2px;
    color: white;
    margin-top: 25px;
    margin-right: 25px;
    margin-bottom: 25px;
    margin-left: 25px;
}

.score-label {
    font-size: 48px;
    color: white;
    margin-top: 20px;
    margin-bottom: 50px;
    -unity-text-align: upper-center;
    width: 625px;
    min-width: 525px;
}

.reset-button {
    font-size: 36px;
    margin-top: 20px;
    width: 200px;
    height: 60px;
}

.show-search-field {
    width: 80%;
    height: 50px;
    margin-top: 20px;
    font-size: 24px;
}

.show-list {
    width: 90%;
    height: 70%;
}

.show-element {
    flex-direction: row;
    margin: 10px;
    padding: 10px;
    background-color: rgba(0, 0, 0, 0.3);
    border-radius: 10px;
}

.show-image {
    width: 100px;
    height: 150px;
    -unity-background-scale-mode: scale-to-fit;
}

.show-name {
    font-size: 36px;
    color: white;
    margin-left: 20px;
    -unity-text-align: middle-left;
}
