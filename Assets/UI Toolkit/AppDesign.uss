.background {
    flex-grow: 1;
}

.MainScreenTemplate {
    flex-grow: 1;
    background-image: url("project://database/Assets/Art/App%20Design/Background.png?fileID=2800000&guid=47eb195e0c6e840598a937409b4ef3e8&type=3#Background");
}

.ScreenTemplate {
    flex-grow: 1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    align-items: center;
    align-content: auto;
}

.back-button {
    position: absolute;
    top: auto;
    left: auto;
    padding: 4px 8px;
    background-color: rgba(51, 51, 51, 0);
    color: white;
    border-radius: 4px;
    background-image: url("project://database/Assets/Art/App%20Design/BackButton.png?fileID=2800000&guid=d16d9c00ad7f343519396b0ba2a77e03&type=3#BackButton");
    width: 150px;
    height: 150px;
    min-width: 150px;
    -unity-background-scale-mode: scale-to-fit;
    min-height: 150px;
    -unity-background-image-tint-color: rgb(248, 248, 248);
    bottom: 24px;
    right: 25px;
}

.back-button:hover {
    -unity-background-image-tint-color: rgb(128, 128, 128);
    scale: 1.05;
}

